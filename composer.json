{
    "name": "idct/symfony-nats-messenger",
    "description": "Symfony NATS (JetStream) Messenger Bridge",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IDCT\\NatsMessenger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "IDCT Bartosz Pacho≈Çek",
            "email": "bartosz+github@idct.tech"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-igbinary": "*",
        "symfony/framework-bundle": "^7.2",
        "symfony/messenger": "^7.2",
        "basis-company/nats": "^1",
        "symfony/uid": "^7.2"
    },
    "suggest": {
        "ext-simdjson": "To use SimdJsonSerializer for better performance when not using igbinary."
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "IDCT\\NatsMessenger\\Tests\\": "tests/"
        }
    }
}
